{
  "name": "TrackJournal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:report": "jest --coverage --reporters=default --reporters=jest-html-reporters --reporters=jest-junit",
    "test:emu": "JEST_USE_EMULATORS=true FIREBASE_USE_EMULATORS=true jest --runInBand",
    "test:emu:report": "JEST_USE_EMULATORS=true FIREBASE_USE_EMULATORS=true jest --runInBand --coverage --reporters=default --reporters=jest-html-reporters --reporters=jest-junit",
    "emulators:start": "firebase emulators:start --only firestore,auth --project demo-track-journal --import=.firebase-data --export-on-exit",
    "emulators:start:clean": "rm -rf .firebase-data && npm run emulators:start",
    "report:open": "open test-reports/html/test-report.html",
    "report:clean": "rm -rf test-reports",
    "report:view": "node scripts/test-report-viewer.js",
    "report:view:qa": "node scripts/test-report-viewer.js qa",
    "report:view:coverage": "node scripts/test-report-viewer.js coverage",
    "report:open:html": "node scripts/test-report-viewer.js open html",
    "report:open:coverage": "node scripts/test-report-viewer.js open coverage"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.4.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native/new-app-screen": "0.81.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "@react-navigation/stack": "^7.4.7",
    "firebase": "^11.2.0",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-gesture-handler": "^2.18.0",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.6.0",
    "react-native-vector-icons": "^10.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.0",
    "@react-native/eslint-config": "0.81.0",
    "@react-native/metro-config": "0.81.0",
    "@react-native/typescript-config": "0.81.0",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "chalk": "^4.1.2",
    "eslint": "^8.19.0",
    "firebase-tools": "^13.22.0",
    "jest": "^29.6.3",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.10",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}
